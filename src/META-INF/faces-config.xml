<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <component>
  <component-type>com.jwl.component.Article</component-type>
  <component-class>com.jwl.presentation.components.article.ArticleComponent</component-class>
 </component>
 <component>
  <component-type>com.jwl.component.Administration</component-type>
  <component-class>com.jwl.presentation.components.administration.AdministrationComponent</component-class>
 </component>
 <component>
  <component-type>com.jwl.component.Search</component-type>
  <component-class>com.jwl.presentation.components.search.SearchComponent</component-class>
 </component>
  <component>
  <component-type>com.jwl.component.Wiki</component-type>
  <component-class>com.jwl.presentation.components.wiki.WikiComponent</component-class>
 </component>
 <component>
  <component-type>com.jwl.component.Widget</component-type>
  <component-class>com.jwl.presentation.components.widget.WidgetComponent</component-class>
 </component>
 <render-kit>
  <!-- ADVANCED HTML RENDERERS -->
  <renderer>
   <component-family>javax.faces.Panel</component-family>
   <renderer-type>com.jwl.html.HeaderPanelGridRenderer</renderer-type>
   <renderer-class>com.jwl.presentation.html.renderer.HeaderPanelGridRenderer</renderer-class>
  </renderer>
  <renderer>
   <component-family>javax.faces.Form</component-family>
   <renderer-type>com.jwl.html.ActionFormRenderer</renderer-type>
   <renderer-class>com.jwl.presentation.html.renderer.ActionFormRenderer</renderer-class>
  </renderer>
  <renderer>
   <component-family>javax.faces.Input</component-family>
   <renderer-type>com.jwl.html.InputFileRenderer</renderer-type>
   <renderer-class>com.jwl.presentation.html.renderer.InputFileRenderer</renderer-class>
  </renderer>
 </render-kit>
 <lifecycle>
   <phase-listener>com.jwl.presentation.knowledgeajax.KnowledgeAjaxPhaseListener</phase-listener>
	</lifecycle>
 <lifecycle>
   <phase-listener>com.jwl.presentation.components.core.AjaxListener</phase-listener>
	</lifecycle>

</faces-config>
